{
  "private": true,
  "scripts": {
    "watch:js": "webpack --watch",
    "watch:css": "node-sass --watch --recursive --output ./assets/dist/stylesheets/ --source-map true ./assets/stylesheets/",
    "watch": "npm-run-all --parallel watch:css watch:js",
    "build:js": "webpack",
    "build:css": "node-sass --recursive --output ./assets/dist/stylesheets/ --source-map true ./assets/stylesheets/",
    "build": "npm-run-all --parallel build:css build:js",
    "font:file": "cp node_modules/font-awesome/fonts/* ./assets/dist/fonts/",
    "font:dir": "mkdir -p ./assets/dist/fonts/",
    "font": "npm-run-all --serial font:dir font:file",
    "start": "npm-run-all --serial font build watch"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "css-loader": "^0.26.1",
    "node-sass": "^4.3.0",
    "npm-run-all": "^4.0.1",
    "ts-loader": "^1.3.1",
    "typescript": "^2.1.4",
    "vue-loader": "^10.0.2",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.1.6",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "bootswatch": "^3.3.7",
    "font-awesome": "^4.7.0",
    "keycode-js": "0.0.4",
    "moment": "^2.17.1",
    "rx": "^4.1.0",
    "vue": "^2.1.6"
  }
}
